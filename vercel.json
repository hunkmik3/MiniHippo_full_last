{
  "version": 2,
  "builds": [
    {
      "src": "minihippofuill/aptiskey.com/api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "minihippofuill/aptiskey.com/**/*",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/minihippofuill/aptiskey.com/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/minihippofuill/aptiskey.com/$1"
    }
  ],
  "cleanUrls": false,
  "trailingSlash": false
}

